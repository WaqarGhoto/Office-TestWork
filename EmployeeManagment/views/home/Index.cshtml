@model IEnumerable<EmployeeManagment.Models.Employee>

   
        <table>
            <thead>
                <tr>
                    <th>ID </th>
                    <th>Name</th>
                    <th>Email</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var employee in Model)
                {
                    <tr>
                        <td>
                            @employee.Id
                        </td>
                        <td>
                            @employee.Name
                        </td>
                        <td>
                            @employee.Email
                        </td>
                    </tr>

                }
            </tbody>
        </table>
